generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model HeyCount {
  id String @id @default(uuid())
  user String
  createdAt DateTime @default(now())
}

model Messages {
  id String @id @default(uuid())
  user String
  message String
  createdAt DateTime @default(now())
}